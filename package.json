{
  "name": "vsc-gameboy",
  "displayName": "vsc-gameboy",
  "description": "game platform for vscode",
  "publisher": "hp-potion",
  "version": "0.2.0",
  "icon": "logo.png",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "GameBoy",
          "title": "it's VSC-GameBoy",
          "icon": "resource/icon/activity-bar-icon.svg"
        }
      ]
    },
    "views": {
      "GameBoy": [
        {
          "id": "gameExplorer",
          "name": "Game Explorer"
        },
        {
          "id": "scoreList",
          "name": "score list"
        }
      ]
    },
    "commands": [
      {
        "command": "vsc-gameboy.openGame",
        "title": "Open Game"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test-util": "mocha"
  },
  "dependencies": {
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "@vscode/test-electron": "^2.3.6"
  }
}
