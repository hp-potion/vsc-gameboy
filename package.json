{
  "name": "vsc-gameboy",
  "displayName": "vsc-gameboy",
  "description": "game platform for vscode",
  "publisher": "hp-potion",
  "version": "0.2.0",
  "icon": "logo.png",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "GameBoy",
          "title": "it's VSC-GameBoy",
          "icon": "resource/icon/activity-bar-icon.svg"
        }
      ]
    },
    "views": {
      "GameBoy": [
        {
          "id": "gameExplorer",
          "name": "Game Explorer"
        },
        {
          "id": "scoreList",
          "name": "Score List"
        }
      ]
    },
    "commands": [
      {
        "command": "vsc-gameboy.openGame",
        "title": "Open Game"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile-old": "tsc -p ./",
    "watch-old": "tsc -watch -p ./",
    "compile": "swc src -d out --source-maps",
    "watch": "swc src -d out --source-maps",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "npm run test-extension && npm run test-util",
    "test-extension": "vscode-test",
    "test-util": "jest"
  },
  "dependencies": {
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.96",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "mocha": "^10.2.0"
  }
}
